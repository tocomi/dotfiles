# alias
alias ezsh='vim ~/.zprofile'
alias szsh='source ~/.zprofile'
alias ezshrc='vim ~/.zshrc'
alias szshrc='source ~/.zshrc'

alias ll='ls -l'
alias la='ls -al'

alias c='clear'

